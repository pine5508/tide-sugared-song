<div class="d-flex justify-content-between">
  <div>
    
    <h1>Welcome, <%= user.googlename %></h1>
  </div>
  <div>
    <a href="/editProfile" class="btn btn-success">Edit</a>
  </div>
</div>

<div class="row">
  <div class="col">
    <img src="<%= user.imageURL %>" width="100%" alt="Profile Picture">
  </div>
  <div class="col">
    <table class="table table-bordered">
      <tbody>
        <tr>
          <td>googlename</td>
          <td><%= user.googlename %> </td>
        </tr>
        <tr>
          <td>googleemail</td>
          <td><%= user.googleemail %> </td>
        </tr>
        <tr>
          <td>username</td>
          <td><%= user.username %> </td>
        </tr>
        <tr>
          <td>age</td>
          <td><%= user.age %> </td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="col">
  </div>
</div>
